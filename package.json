{
  "name": "node-efficientnet",
  "version": "2.0.4",
  "description": "Implementation of efficientNet model in nodejs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "rm -rf ./dist && tsc &&  cp -r src/misc ./dist/src/misc",
    "example": "ts-node example.ts",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "test": "npm run test:prapare && npm run test:unit",
    "lint": "eslint . --fix --ext .ts && npm run lint:md",
    "lint:md": "remark .",
    "test:unit": "jest  --clearCache && jest  --coverage",
    "test:prapare": "npm link && cd tester && npm link node-efficientnet",
    "test:playground": "cd playground/server && npm i && npm test",
    "deploy:playground": "sh deploy.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntedgi/node-efficientnet.git"
  },
  "author": "Naor Tedgi (Naor.tedgi@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ntedgi/node-efficientnet/issues"
  },
  "homepage": "https://github.com/ntedgi/node-efficientnet#readme",
  "dependencies": {
    "@tensorflow/tfjs-core": "^3.3.0",
    "@tensorflow/tfjs-node-gpu": "^3.3.0",
    "@types/cli-progress": "^3.8.0",
    "cli-progress": "^3.8.2",
    "jimp": "^0.16.1",
    "node-fetch": "^2.6.1",
    "ts-node": "^9.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.19",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "regenerator-runtime": "^0.13.7",
    "remark-cli": "^9.0.0",
    "remark-lint": "^8.0.0",
    "remark-lint-emphasis-marker": "^2.0.1",
    "remark-lint-strong-marker": "^2.0.1",
    "remark-preset-lint-recommended": "^5.0.0",
    "shelljs": "^0.8.4",
    "typescript": "^4.1.3"
  },
  "jest": {
    "transform": {}
  },
  "remarkConfig": {
    "settings": {
      "emphasis": "*",
      "strong": "*"
    },
    "plugins": [
      "remark-preset-lint-recommended",
      "remark-lint-emphasis-marker",
      "remark-lint-strong-marker",
      "remark-stringify"
    ]
  },
  "keywords": [
    "tensorflow",
    "tfjs",
    "efficientnet",
    "imagenet",
    "object detection",
    "dnn"
  ]
}